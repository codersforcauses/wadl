{
  "name": "wadl",
  "author": "Coders for Causes <hello@codersforcauses.org> (https://codersforcauses.org/)",
  "description": "A progressive web-application to assist the Western Australian Debating League with their seasonal organisation needs.",
  "homepage": "https://codersforcauses.github.io/wadl/",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "dev:emu": "yarn firebase & (sleep 15 && yarn dev)",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore --fix .",
    "firebase": "yarn emu:stop & cd Firebase && firebase emulators:start --import=dummyData",
    "emu:stop": "lsof -ti :9099 -ti :5001 -ti :8080 -ti :8085  -ti :9199 | xargs kill",
    "dev:docker": "docker-compose up"
  },
  "devDependencies": {
    "@kevinmarrec/nuxt-pwa": "^0.15.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.8.0",
    "nuxt": "3.0.0",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@google-cloud/billing": "^3.1.3",
    "@heroicons/vue": "^2.0.13",
    "@pinia/nuxt": "^0.4.6",
    "@vueuse/components": "^9.10.0",
    "@vueuse/core": "^9.10.0",
    "",
    "firebase": "^9.14.0",
    "firebase-functions": "^4.1.0",
    "pinia": "^2.0.27